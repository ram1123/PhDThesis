@MastersThesis{Dubouchet2013,
  author   = {Dubouchet, Frédéric},
  title    = {Betatron tune measurement with the LHC damper using a GPU},
  year     = {2013},
  note     = {CERN-THESIS-2013-035},
  abstract = {This thesis studies a possible futur implementation of a betatron tune measure- ment in the Large Hadron Collider (LHC) at European organization for nuclear research (CERN) using a General Purpose Graphic Processing Unit (GPGPU) to analyse data acquired with the LHC transverse transverse damper (ADT). The present hardware and future possible implementations using ADT acquisi- tions and Graphic Processing Unit (GPU) computing are described. The ADT data have to be processed to extract the betatron tune. To compute the tune, the signal is transformed from the time domain to the frequency domain using Fast Fourier Transform (FFT) on GPUs. We show that it is possible to achieve one order of magnitude faster FFTs on a Fermi generation GPU than what can be done using a i7 generation Central Processing Unit (CPU). This makes online per bunch FFT computation and betatron tune measurement possible.},
  file     = {:/Users/ramkrishna/cernbox/ThesisReference_2018/LHC/Betatron-Tune-CERN-THESIS-2013-035.pdf:PDF},
  url      = {https://cds.cern.ch/record/1545785?ln=en},
}

